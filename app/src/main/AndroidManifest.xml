<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.fivefivelike.androidprojectdemo">

    <uses-permission android:name="android.permission.SET_WALLPAPER"/>

    <application
        android:name=".BaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".mvp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".mvp.activity.RollViewActivity"/>
        <activity android:name=".mvp.activity.HeadExpandActivity"/>
        <activity android:name=".mvp.activity.ImageChooseActivity"/>
        <activity android:name=".mvp.activity.DialogActivity"/>
        <activity android:name=".mvp.activity.TablayoutActivity"/>
        <activity android:name=".mvp.activity.SegmentTabActivity"/>
        <activity android:name=".mvp.activity.SlidingTabActivity"/>
        <activity android:name=".mvp.activity.CommonTabActivity"/>
        <activity android:name=".mvp.activity.RequestActivity"/>
        <activity android:name=".mvp.activity.WaveBarActivity"/>
        <activity android:name=".mvp.activity.TestActivity"/>
        <activity android:name=".mvp.activity.TestViewActivity"/>
        <activity android:name=".mvp.activity.NormalWebviewActivityActivity"/>
        <activity android:name=".mvp.activity.ToastActivity"/>
    </application>

</manifest>